pipeline{
    agent{
        label "maven"
    }
    environment {
        PATH = "/opt/apache-maven-3.9.4/bin:$PATH"
    }
    stages{
        stage("BUILD"){
            steps{
                echo "BUILD start ---------"
                sh 'mvn clean deploy'
                echo "BUILD end------"
            }
            
        }
    }
}
 
