pipeline{
    agent{
        label "maven"
    }
    stages{
        stage("clone code"){
            steps{
                echo "cloning start ---------"
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/badshahgit/tweet-trend-new.git']])
                echo "cloning end------"
            }
            
        }
    }
}
  